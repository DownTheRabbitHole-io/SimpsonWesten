{
	"name": "silverstripe/installer",
	"description": "The SilverStripe Framework Installer",
	"require": {
		"php": ">=5.3.3",
		"silverstripe/cms": "3.5.*",
		"silverstripe/framework": "3.5.*",
		"silverstripe/reports": "3.5.*",
		"silverstripe/siteconfig": "3.5.*",
		"silverstripe-themes/simple": "3.1.*",
		"silverstripe/blog": "2.4.x",
		"thezenmonkey/silverstripe-amp": "dev-master",
		"undefinedoffset/sortablegridfield": "^0.6.9"
	},
	"require-dev": {
		"phpunit/PHPUnit": "~3.7@stable"
